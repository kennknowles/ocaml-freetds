(jbuild_version 1)

(executables
 ((names (test_dblib test_ct))
  (libraries (freetds oUnit threads))))

(alias
 ((name test)
  (deps (test_dblib.exe test_ct.exe))))

(alias
 ((name runtest)
  (deps (test_dblib.exe))
  (action (run ${<}))))

(alias
 ((name runtest)
  (deps (test_ct.exe))
  (action (run ${<}))))
